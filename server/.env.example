# Environment variables declared in this file are NOT automatically loaded by Prisma.
# Please add `import "dotenv/config";` to your `prisma.config.ts` file, or use the Prisma CLI with Bun
# to load environment variables from .env files: https://pris.ly/prisma-config-env-vars.

# Prisma supports the native connection string format for PostgreSQL, MySQL, SQLite, SQL Server, MongoDB and CockroachDB.
# See the documentation for all the connection string options: https://pris.ly/d/connection-strings

DATABASE_URL="file:./dev.db"
GITHUB_TOKEN=ghp_your_github_token_here
GITHUB_OWNER=your_github_username
GITHUB_OWNER_ALLOWLIST=your_github_username,another_user
VERCEL_TOKEN=your_vercel_token_here
VERCEL_TEAM_ID=

SUPABASE_ACCESS_TOKEN=sbp_your_supabase_token_here

# Dev Hub Fastify server configuration
PORT=4000
API_ADMIN_TOKEN=your_secure_admin_token_here

# Optional Supabase management API config (used by integrations summary)
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# Hugging Face configuration (used by AI features)
HF_API_KEY=hf_your_huggingface_key_here
HF_API_URL=https://router.huggingface.co/v1/chat/completions
HF_MODEL_ID=meta-llama/Meta-Llama-3.1-8B-Instruct

# Optional: comma-separated hostnames allowed for API Explorer proxy,
# e.g. "api.github.com,api.openai.com"
API_PROXY_ALLOWLIST=
